pipeline {
    agent {
        dockerfile { 
            filename 'Dockerfile.jenkins'
            registryUrl 'https://quay.io/repository/'
            registryCredentialsId 'quay-welltok-mkitawat'
        }
    }
    stages {
        stage('Practice1') {
            steps {
                sh 'whereis python'
                sh 'whereis pip'
                sh 'python --version'
                sh 'pip --version'
                sh 'ls -la /usr/local/bin/python'
                sh 'pip show Django'
            }
        }
    }
}